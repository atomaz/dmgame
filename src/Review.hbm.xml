<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping.dtd	">

<hibernate-mapping>

	<class name="model.Review" table="game">

		<id name="id" column="id" type="long">

			<generator class="assigned" />

		</id>
		
		<property name="description" column="description" type="string" />
		
		<property name="url" type="string" />
		
		<property name="grade" type="double" />
		
		<many-to-one name="game" class="model.Game"/>

	</class>

</hibernate-mapping>



