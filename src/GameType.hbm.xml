<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping.dtd">

<hibernate-mapping>

	<class name="model.GameType" table="gametype">

		<id name="id" column="id" type="long">

			<generator class="increment" />

		</id>

		<property name="name" type="string" column="gt_name" />

	</class>

</hibernate-mapping>



